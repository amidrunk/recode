{
	"name": "recode-site",
	"version": "0.0.1",
	"scripts": {
		"build": "npm run _webpack && npm run _static",
		"_webpack": "./node_modules/webpack/bin/webpack.js",
		"_static": "./node_modules/copyfiles/copyfiles -u 3 ./src/main/site/** ./src/main/site/**/* target/classes/",
		"dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base src/main/site --output-file \"/[name].js\""
	},
	"dependencies": {
		"react-router": "2.6.0",
		"react": "15.2.1",
		"react-dom": "15.2.1",
		"jquery": "2.2.4"
	},
	"devDependencies": {
		"webpack": "1.13.1",
		"webpack-dev-server": "1.14.1",
		"babel-loader": "6.2.4",
		"babel-preset-es2015": "6.9.0",
		"babel-preset-react": "6.11.1",
		"script-loader": "0.7.0",
		"copyfiles": "1.0.0"
	}
}
